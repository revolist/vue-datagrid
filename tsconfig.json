{
  "compilerOptions": {
    "outDir": "./dist/",
    "baseUrl": "./",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "allowUnreachableCode": true,
    "declaration": true,
    "experimentalDecorators": true,
    "lib": ["dom", "es2017", "ESNext"],
    "moduleResolution": "node",
    "module": "esnext",
    "target": "ES5",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "sourceMap": true,
    "paths": {
      "@revolist/vue-datagrid": ["lib"]
    }
  },
  "include": ["types/jsx.d.ts", "lib", "/**/*.d.ts", "/**/*.ts", "*.d.ts"],
  "exclude": ["node_modules", "demo-v2.7", "demo"]
}
